<script>
/*
 * JS: Blind mode
 */

var bBtn = '#btn-sand-blind';
var bKey = 'sand_blind_mode_exp';

function blindModeOff() {
 	$('body').addClass('blind_mode');
    $(bBtn).html('<span>Включить обычную версию</span>');
    $('img.nm-plan').hide();
    $('img.bd-plan').show();
}

function blindModeOn() {
	$('body').removeClass('blind_mode');
 	$(bBtn).html('<span>Включить версию для слабовидящих</span>');
    $('img.nm-plan').show();
    $('img.bd-plan').hide();
}

function setModeNormal() {
	blindModeOn();
}

function setModeBlind() {
	blindModeOff();
}

$( document ).ready(function() {
	var bMode = getCookie(bKey);
    if (bMode == undefined) {
    	setModeNormal();
    } else if (bMode == 'on') {
    	setModeBlind();
    } else {
    	setModeNormal();
    }
    $(bBtn).click(changeBlindMode);
});

function changeBlindMode() {
	var bMode = getCookie(bKey);
 	if (bMode == undefined) {
 		addCookie(bKey, 'on');
 		setModeBlind();
 	} else {
 		if (bMode == 'on') {
 			addCookie(bKey, 'off');
 			setModeNormal();
 		} else {
 			addCookie(bKey, 'on');
 			setModeBlind();
 		}
 	}
}

function getCookie(name) {
	var matches = document.cookie.match(new RegExp(
    	"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
	));
	if(matches == null) {
	    return undefined;
	}
	console.log(matches);
	console.log(decodeURIComponent(matches[1]));
	return matches ? decodeURIComponent(matches[1]) : undefined;
}

function removeCookie(name) {
	updateCookieWithExpires(name, 'off', -2);
}

function addCookie(name, value) {
	updateCookieWithExpires(name, value, 2);
}

function updateCookieWithExpires(name, value, days) {
	var d = new Date();
	d.setDate((new Date()).getDate() + days);

	options = {
		expires: d.toUTCString(),
		path: '/'
	};
	updateCookie(name, value, options);
}

function updateCookie(name, value, options) {
	value = encodeURIComponent(value);
  	var updatedCookie = name + "=" + value;

  	for (var propName in options) {
    	updatedCookie += "; " + propName;
    	var propValue = options[propName];
    	if (propValue !== true) {
      		updatedCookie += "=" + propValue;
    	}
  	}
	document.cookie = updatedCookie;
}

					</script>
